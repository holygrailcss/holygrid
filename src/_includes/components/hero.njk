<div class="container-full ">

  <div class="row section-x height-xl align-middle interior ">
    <div class="col-xs-12 col-sm-6 front-text mdmax:mb-42">
      <div class=" ">
        <div class="vertical-wrap">
          <div class="vertical-inner">
            <h1 class="fluid-2-b pb-40">{{ title }}</h1>
          </div>
        </div>
        <div class="vertical-wrap-2">
          <div class="vertical-inner">
            <h2 class="fluid-7-b mb-8">{{ subtitle }}</h2>
          </div>
        </div>

        <div class="vertical-wrap">
          <div class="vertical-inner">
            <span class="text-14-r pb-40"> {{ fecha }} </span>
          </div>
        </div>
      </div>
    </div>
    <div class="col-xs-12 col-sm-6 mdmax:mb-42">
      {% if imagemini %}
      <div class="rat-16-9">
        <div class="rat-content">
          <img class="video-cover" width="100%" height="100%" src="{{ imagemini }}" alt="{{ imageAlt }}" />
        </div>
      </div>
      {% elseif image %}
      <div class="rat-16-9">
        <div class="rat-content">
          <img class="video-cover" width="100%" height="100%" src=" {{ image }}" alt="{{ imageAlt }}" />
        </div>
      </div>
      {% endif %}
    </div>
  </div>
</div>
<div class="container-full">
  <div class="col-xs-12">{% include 'components/breadcrumbs.njk' %}</div>
</div>
{% if duracion %}
<div class="container-2 pb-32">
  <div class="row">
    <div class="col-xs-12 col-sm-12 content-center is-flex">
      <div class="mr-32">
        <div class="duration text-center">
          <span>DURACIÃ“N</span> {{ duracion }}
        </div>
      </div>
      <div class="fluid-32-balign-middle">{{ descripcion }}</div>
    </div>
  </div>
</div>
{% else %}
<div class="mdmax:mb-42 pb-64"></div>
{% endif %}
